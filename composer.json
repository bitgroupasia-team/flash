{
    "name": "bitgroupasia/flash",
    "description": "Laravel flash",
    "license": "MIT",
    "authors": [
        {
            "name": "aepnat",
            "email": "aep.nat@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Bitgroupasia\\Flash\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Bitgroupasia\\Flash\\FlashServiceProvider"
            ],
            "aliases": {
                "Flash": "Bitgroupasia\\Flash\\Facades\\Flash"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
